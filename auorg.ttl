<http://test.linked.data.gov.au/def/auorg> rdf:type owl:Ontology ;
                                            owl:versionIRI <http://test.linked.data.gov.au/def/auorg/0.5> ;
                                            owl:imports <http://www.w3.org/ns/org#> ;
                                            <http://purl.org/dc/terms/creator> "Nicholas Car"^^xsd:string ;
                                            rdfs:label "AU Org Ontology"@en ;
                                            <http://purl.org/dc/terms/modified> "2018-05-21"^^xsd:date ;
                                            owl:versionInfo "Beta version"@en ;
                                            <http://purl.org/dc/terms/created> "2018-05-21"^^xsd:date ;
